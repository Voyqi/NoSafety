function FixCall()
    game:GetService("ReplicatedStorage").Mobile.MobileReceiveEvent:FireServer()

    OrionLib:MakeNotification({
        Name = "Successfully",
        Content = "Disconnected [Phone Call]!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

    

    -- PlayerTab
    local PlayerTab = Window:MakeTab({
        Name = "PLAYER",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

            --PlayerButtons
            PlayerTab:AddButton({
                Name = "MOD Notifier",
                Callback = function()
                          MODDetection()
                  end    
            })

            PlayerTab:AddButton({
                Name = "Call Plug",
                Callback = function()
                          CallPlug()
                  end    
            })
        
            PlayerTab:AddButton({
                Name = "No Cooldown",
                Callback = function()
                          NoCooldown()
                  end    
            })


            --PlayerToggles
    PlayerTab:AddToggle({
        Name = "Infinite Stamina",
        Default = false,
        Callback = function(Value)
        _G.InfiniteStamina = Value
        InfiniteStamina()
        end
    })

    -- TeleportsTab
    local TeleportTab = Window:MakeTab({
        Name = "TELEPORTS",
