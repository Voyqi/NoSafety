    function InfiniteStamina()
        while _G.InfiniteStamina == true do
    
    Wait (0.1)
    
    local lname = game.Players.LocalPlayer.Name
    game:GetService("ReplicatedStorage").PlayerData[lname].PlayerAdditions.Stamina.Value = 50
    
